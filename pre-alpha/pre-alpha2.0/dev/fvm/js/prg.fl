INT_MAX. 2147483647

bar{
  go :calc

  bad1:
    halt

  calc:
    onFail :bad1
    foo
    \ .INT_MAX 1 + \ Addition will branch to onFail due to overflow
    3 5 +
    halt

  bad2:
    halt

  : foo
    onFail bar{::bad2} \ Exactly the same as onFail ::bad2
    {..INT_MAX} 4 +
  ;

}bar

